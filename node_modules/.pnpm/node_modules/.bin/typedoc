#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/k8s/ocm-engine/ocm-engine/node_modules/.pnpm/typedoc@0.22.18_typescript@4.9.5/node_modules/typedoc/bin/node_modules:/home/ubuntu/k8s/ocm-engine/ocm-engine/node_modules/.pnpm/typedoc@0.22.18_typescript@4.9.5/node_modules/typedoc/node_modules:/home/ubuntu/k8s/ocm-engine/ocm-engine/node_modules/.pnpm/typedoc@0.22.18_typescript@4.9.5/node_modules:/home/ubuntu/k8s/ocm-engine/ocm-engine/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/k8s/ocm-engine/ocm-engine/node_modules/.pnpm/typedoc@0.22.18_typescript@4.9.5/node_modules/typedoc/bin/node_modules:/home/ubuntu/k8s/ocm-engine/ocm-engine/node_modules/.pnpm/typedoc@0.22.18_typescript@4.9.5/node_modules/typedoc/node_modules:/home/ubuntu/k8s/ocm-engine/ocm-engine/node_modules/.pnpm/typedoc@0.22.18_typescript@4.9.5/node_modules:/home/ubuntu/k8s/ocm-engine/ocm-engine/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typedoc/bin/typedoc" "$@"
else
  exec node  "$basedir/../typedoc/bin/typedoc" "$@"
fi
